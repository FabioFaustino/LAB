Square square1 = new Square();
square1.makeVisible();
square1.changeSize(180);
square1.moveLeft();
square1.moveUp();
square1.slowMoveVertical(-100);
square1.slowMoveHorizontal(-290);
square1.changeColor("green");
Square square2 = new Square();
square2.makeVisible();
square2.changeSize(120);
square2.slowMoveHorizontal(40);
square2.moveRight();
square2.slowMoveHorizontal(10);
square2.slowMoveVertical(30);
square2.moveDown();
square2.slowMoveVertical(5);
square2.slowMoveVertical(5);
square2.slowMoveVertical(-2);
square2.slowMoveHorizontal(-2);
square2.changeColor("blue");
square1.slowMoveVertical(5);
square1.slowMoveHorizontal(5);
square2.slowMoveHorizontal(-3);
square2.slowMoveVertical(-3);
