Square square1 = new Square();
square1.makeVisible();
square1.changeSize(180);
square1.moveVertical(-150);
square1.moveHorizontal(-250);
square1.moveHorizontal(-50);
square1.moveVertical(10);
square1.slowMoveVertical(10);
square1.slowMoveVertical(10);
square1.slowMoveVertical(10);
square1.slowMoveVertical(-5);
square1.slowMoveHorizontal(-5);
square1.changeColor("green");
